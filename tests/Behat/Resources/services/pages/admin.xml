<?xml version="1.0" encoding="UTF-8"?>

<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="app.behat.page.admin.product.create_simple.class">App\Tests\Behat\Page\Admin\Product\CreateSimpleProductPage</parameter>
        <parameter key="app.behat.page.admin.product.update_simple.class">App\Tests\Behat\Page\Admin\Product\UpdateSimpleProductPage</parameter>
    </parameters>

    <services>
        <defaults public="true"/>

        <service id="app.behat.page.admin.product.create_simple"
                 class="%app.behat.page.admin.product.create_simple.class%"
                 parent="sylius.behat.page.admin.product.create_simple" public="false">
        </service>
        <service id="app.behat.page.admin.product.update_simple"
                 class="%app.behat.page.admin.product.update_simple.class%"
                 parent="sylius.behat.page.admin.product.update_simple" public="false">
        </service>
    </services>
</container>
